#include "monty.c"

/**
 * pstr -
 *
 * @stack: stack
 * @line_number: line number in file
 *
 * Return: void
 */
void pstr(stack_t **stack, unsigned int line_number)
{
if (stack == NULL)
{
printf("\n");
return;
}

stack_t *current = stack;
while (current != NULL && current->n != 0 && current->n >= 0 && current->n <= 127)
{
printf("%c", current->n);
current = current->next;
}

printf("\n");
}
